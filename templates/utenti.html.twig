{# utenti.html.twig #}
{% extends "base.html.twig" %}

{% block body %}
	<table>
		<tr>
			<th>Nome</th>
			<th>Cognome</th>
			<th>Email</th>
		</tr>
		{% for utente in utenti %}
	  		<tr>
	  			<td>{{ utente.nome }}</td>
	  			<td>{{ utente.cognome }}</td>
	  			<td>{{ utente.email }}</td>
	  		</tr>
		{% endfor %}
	</table>
{% endblock %}